{
    "manifest_version": 2,
  
    "name": "Hide Name",
    "description": "Hide your exes name on any website",
    "version": "1.0",
  
    "browser_action": {
     "default_icon": "icon.png",
     "default_popup": "popup.html"
    },
    "background": {
        "scripts": ["./popup.js"]
    },
    "permissions": [
     "activeTab",
     "tabs"
     ],
     "content_security_policy": "script-src 'self' https://ajax.googleapis.com; object-src 'self'"
  }